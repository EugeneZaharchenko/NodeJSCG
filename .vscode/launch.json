{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Docker: Attach to Node",
        "remoteRoot": "/app",
        "port": 9229,
        "address": "localhost",
        "localRoot": "${workspaceFolder}",
        "restart": true,
        "sourceMaps": true,
        "skipFiles": [
          "${workspaceFolder}/node_modules/**/*.js",
          "<node_internals>/**/*.js"
        ]
      }
    ]
  }